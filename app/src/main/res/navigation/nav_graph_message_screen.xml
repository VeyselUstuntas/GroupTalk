<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_message_screen"
    app:startDestination="@id/messages">

    <fragment
        android:id="@+id/messages"
        android:name="com.vustuntas.grouptalk.fragment.Messages"
        android:label="fragment_messages"
        tools:layout="@layout/fragment_messages" >
        <action
            android:id="@+id/action_messages_to_usersList"
            app:destination="@id/usersList" />
    </fragment>
    <fragment
        android:id="@+id/usersList"
        android:name="com.vustuntas.grouptalk.fragment.UsersList"
        android:label="fragment_users_list"
        tools:layout="@layout/fragment_users_list" />
</navigation>